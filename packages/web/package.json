{
  "name": "@any-reader/web",
  "version": "1.7.0",
  "private": true,
  "scripts": {
    "dev": "cross-env VITE_APP_PLATFORM=browser vite",
    "build:w": "cross-env VITE_APP_PLATFORM=vscode vite build --watch",
    "build": "cross-env VITE_APP_PLATFORM=vscode run-p type-check \"build-only {@}\" --",
    "build:b": "cross-env VITE_APP_PLATFORM=browser run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "electron:dev": "cross-env VITE_APP_PLATFORM=electron vite",
    "electron:build-tpl": "cross-env VITE_APP_PLATFORM=electron run-p type-check \"build-only {@}\" --",
    "electron:build-app": "electron-builder",
    "electron:build": "npm-run-all electron:build-tpl electron:build-app",
    "vscode:dev": "cross-env VITE_APP_PLATFORM=vscode vite",
    "vscode:build": "cross-env VITE_APP_PLATFORM=vscode run-p type-check \"build-only {@}\" --",
    "cli:build": "cross-env VITE_APP_PLATFORM=browser outDir=../cli/public VITE_APP_BASE_URL=/ vite build",
    "postinstall": "electron-builder install-app-deps"
  },
  "main": "dist/electron-js/main.js",
  "dependencies": {
    "@any-reader/core": "workspace:^",
    "@any-reader/rule-utils": "workspace:^",
    "@any-reader/shared": "workspace:^",
    "@arco-design/web-vue": "^2.55.2",
    "@unocss/reset": "^0.51.13",
    "@vscode/codicons": "^0.0.36",
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "@vueuse/core": "^10.9.0",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "blueimp-md5": "^2.19.0",
    "dplayer": "^1.27.1",
    "easy-post-message": "^0.1.0",
    "file-saver": "^2.0.5",
    "hls.js": "^1.5.3",
    "lodash-es": "^4.17.21",
    "monaco-editor": "0.47.0",
    "nprogress": "^0.2.0",
    "p-limit": "^5.0.0",
    "pinia": "^2.1.7",
    "qs": "^6.12.1",
    "uuid": "^9.0.1",
    "vue": "^3.4.15",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/blueimp-md5": "^2.18.2",
    "@types/jsdom": "^21.1.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.16",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "chalk": "^5.2.0",
    "cross-env": "^7.0.3",
    "electron": "30.0.6",
    "electron-builder": "^24.13.3",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "mockjs": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "sass": "^1.59.3",
    "sass-loader": "^13.2.1",
    "typescript": "~5.3.3",
    "unocss": "^0.58.4",
    "unplugin-auto-import": "^0.16.4",
    "vite": "^5.0.13",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-mock": "^3.0.1",
    "vue-tsc": "^1.8.27"
  }
}
