  "homepage": "https://github.com/aooiuu/any-reader#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/aooiuu/any-reader"
  },
  "bugs": {
    "url": "https://github.com/aooiuu/any-reader/issues"
  },
  "private": true,
  "keywords": [],
  "scripts": {
    "dev": "cross-env VITE_APP_PLATFORM=browser vite",
    "build:w": "cross-env VITE_APP_PLATFORM=vscode vite build --watch",
    "build": "cross-env VITE_APP_PLATFORM=vscode run-p type-check \"build-only {@}\" --",
    "build:b": "cross-env VITE_APP_PLATFORM=browser run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "electron:dev": "cross-env VITE_APP_PLATFORM=electron vite",
    "electron:build-tpl": "cross-env VITE_APP_PLATFORM=electron run-p type-check \"build-only {@}\" --",
    "electron:build-app": "electron-builder",
    "electron:build": "npm-run-all electron:build-tpl electron:build-app",
    "vscode:dev": "cross-env VITE_APP_PLATFORM=vscode vite",
    "vscode:build": "cross-env VITE_APP_PLATFORM=vscode run-p type-check \"build-only {@}\" --",
    "utools:build": "cross-env VITE_APP_PLATFORM=utools run-p type-check \"build-only {@}\" --",
    "cli:build": "cross-env VITE_APP_PLATFORM=browser outDir=../cli/public vite build",
    "postinstall": "electron-builder install-app-deps",
    "rebuild": "electron-rebuild -f -w sqlite3"
  },
  "main": "dist/electron-js/main.js",
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@any-reader/core": "workspace:^",
    "@any-reader/rule-utils": "workspace:^",
    "@any-reader/shared": "workspace:^",
    "@unocss/reset": "^0.51.13",
    "@v-c/utils": "^0.0.26",
    "@vscode/codicons": "^0.0.36",
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "@vueuse/core": "^10.9.0",
    "ant-design-vue": "^4.2.3",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "blueimp-md5": "^2.19.0",
    "dplayer": "^1.27.1",
    "easy-post-message": "^0.1.0",
    "electron-localshortcut": "^3.2.1",
    "electron-updater": "^6.2.1",
    "file-saver": "^2.0.5",
    "hls.js": "^1.5.3",
    "lodash-es": "^4.17.21",
    "monaco-editor": "0.47.0",
    "nprogress": "^0.2.0",
    "p-limit": "^5.0.0",
    "pinia": "^2.1.7",
    "qs": "^6.12.1",
    "sqlite3": "5.1.6",
    "uuid": "^9.0.1",
    "vue": "^3.4.15",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/blueimp-md5": "^2.18.2",
    "@types/electron-localshortcut": "^3.1.3",
    "@types/file-saver": "^2.0.7",
    "@types/jsdom": "^21.1.6",
    "@types/lodash-es": "^4.17.12",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-config": "^0.61.9",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "chalk": "^5.2.0",
    "electron": "30.0.6",
    "electron-builder": "^24.13.3",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "mockjs": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "sass": "^1.59.3",
    "sass-loader": "^13.2.1",
    "typescript": "^5.5.3",
    "unocss": "^0.58.4",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-vue-components": "^0.27.2",
    "vite": "^5.3.1",
    "vite-bundle-analyzer": "^0.10.3",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-mock": "^3.0.1",
    "vue-tsc": "^2.0.21"
  }
}
