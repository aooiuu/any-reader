{
  "name": "@any-reader/vscode",
  "displayName": "any-reader",
  "description": "摸鱼,自定义规则多站点聚合搜索阅读小说、漫画。包含JS规则解析库和VSCode插件。支持本地小说 TXT、EPUB",
  "icon": "resources/icon.png",
  "version": "1.8.0",
  "publisher": "aooiu",
  "private": true,
  "qna": "https://github.com/aooiuu/any-reader/issues",
  "bugs": {
    "url": "https://github.com/aooiuu/any-reader/issues",
    "email": "aooiu@qq.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aooiuu/any-reader"
  },
  "keywords": [
    "阅读",
    "小说",
    "漫画",
    "摸鱼",
    "reader",
    "eso"
  ],
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:any-reader",
    "onWebviewPanel:any-reader"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "build:rename": "esno ./scripts/publish.cjs",
    "build:vsce": "vsce package --no-dependencies",
    "build": "npm-run-all build:rename build:vsce",
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "contributes": {
    "commands": [
      {
        "command": "any-reader.search",
        "title": "搜索",
        "icon": "$(search)",
        "category": "any-reader"
      },
      {
        "command": "any-reader.discover",
        "title": "发现页",
        "icon": "$(flame)",
        "category": "any-reader"
      },
      {
        "command": "any-reader.home",
        "title": "首页",
        "icon": "$(home)",
        "category": "any-reader"
      },
      {
        "command": "any-reader.openLocalBookDir",
        "title": "打开本地文件目录",
        "icon": "$(folder-opened)",
        "category": "any-reader"
      },
      {
        "command": "any-reader.openUrl",
        "title": "打开URL",
        "category": "any-reader"
      },
      {
        "command": "any-reader.openFile",
        "title": "打开文件",
        "category": "any-reader"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "any-reader",
          "title": "any-reader",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "menus": {
      "explorer/context": [
        {
          "command": "any-reader.openFile",
          "when": "resourceExtname == .epub",
          "group": "navigation"
        }
      ]
    },
    "views": {
      "any-reader": [
        {
          "id": "any-reader-webview",
          "name": "view",
          "type": "webview"
        }
      ]
    },
    "customEditors": [
      {
        "viewType": "any-reader.customEditor.epub",
        "displayName": "打开 Epub",
        "selector": [
          {
            "filenamePattern": "*.epub"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "devDependencies": {
    "@any-reader/core": "workspace:^",
    "@any-reader/shared": "workspace:^",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.68.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vscode/test-electron": "^2.1.5",
    "easy-post-message": "^0.1.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "execa": "^8.0.1",
    "explorer-opener": "^1.0.1",
    "fs-extra": "^11.1.1",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "qs": "^6.12.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "uuid": "^9.0.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-filter-warnings-plugin": "^1.2.1"
  },
  "dependencies": {
    "sqlite3": "5.1.6"
  }
}
