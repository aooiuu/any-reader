{
  "$schema": "https://json.schemastore.org/prettierrc",
  "eslintIntegration": true,
  "printWidth": 150,
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "semi": true,
  "trailingComma": "none",
  "bracketSpacing": true,
  "jsxBracketSameLine": true,
  "arrowParens": "always",
  "endOfLine": "lf"
}
